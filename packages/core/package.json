    "@types/mustache": "^4.2.5",
    "debug": "^4.4.0",
    "inquirer": "^12.4.3",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "nanoid": "^5.1.3",
    "openai": "^4.86.2",
    "ora": "^8.2.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.3"
  },
  "peerDependencies": {
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.79.1"
  },
