  },
  "devDependencies": {
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "detect-port": "^2.1.0",
    "hono": "^4.9.7",
    "playwright": "^1.55.0",
